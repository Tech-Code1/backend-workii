  "scripts": {
    "dev:api": "nx serve api",
    "pretty": "npx pretty-quick --pattern \"src/**/*.*(ts|json|html|scss)\" --staged",
    "eslint:check": "nx exec -- eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint": "nx lint",
    "precommit": "npm run pretty && npm run lint",
    "git:config": "git config --local include.path ../.gitconfig",
    "prebuild": "rimraf dist",
    "build": "nx build",
    "format": "nx exec -- prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nx serve",
    "start:debug": "nx serve --configuration=debug",
    "start:prod": "node dist/main",
    "test": "nx test",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js -d config/ormconfig.ts",
    "make:migrate": "npm run typeorm -- migration:generate",
    "make:seed": "ts-node -- tsconfig-paths/register ts-node node_modules/typeorm-extension/dist/cli/index.js seed",
    "migrate:run": "npm run typeorm migration:run",
    "migrate:revert": "npm run typeorm migration:revert",
    "e2e": "nx e2e"
  },
  "dependencies": {
    "@commitlint/config-conventional": "^17.6.3",
